<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet><servlet-name>login</servlet-name>
        <servlet-class>com.example.Lab_9.LogInServlet</servlet-class>
    </servlet>
    <servlet-mapping><servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet><servlet-name>logout</servlet-name>
        <servlet-class>com.example.Lab_9.LogOutServlet</servlet-class>
    </servlet>
    <servlet-mapping><servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>

    <servlet><servlet-name>dashboard</servlet-name>
        <servlet-class>com.example.Lab_9.DashboardServlet</servlet-class>
    </servlet>
    <servlet-mapping><servlet-name>dashboard</servlet-name>
        <url-pattern>/dashboard</url-pattern>
    </servlet-mapping>

    <servlet><servlet-name>getcities</servlet-name>
        <servlet-class>com.example.Lab_9.GetDirectCitiesServlet</servlet-class>
    </servlet>
    <servlet-mapping><servlet-name>getcities</servlet-name>
        <url-pattern>/get-cities</url-pattern>
    </servlet-mapping>
</web-app>